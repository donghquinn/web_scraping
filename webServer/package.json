{
  "name": "scraping",
  "version": "2.0.0",
  "main": "index.js",
  "license": "MIT",
  "author": "donghquinn",
  "packageManager": "yarn@3.5.1",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node --max-old-space-size=700 -r ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node  ./src/main.ts",
    "start:dev": "NODE_ENV=DEVELOPMENT nest start --watch",
    "build": "NODE_ENV=production tsc --project tsconfig.json --diagnostics  -p tsconfig.json",
    "dev": "NODE_ENV=development node --max-old-space-size=700 -r ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node  ./src/main.ts",
    "dev:bbc": "nodemon --exec node -r  ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node ./src/libraries/scrape/bbc.lib.ts",
    "dev:melon": "nodemon --exec node -r  ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node ./src/libraries/scrape/music.lib.ts",
    "dev:climate": "nodemon --exec node -r  ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node ./src/libraries/scrape/climate.lib.ts",
    "dev:naverNews": "nodemon --exec node -r  ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node ./src/libraries/scrape/naver.lib.ts",
    "dev:metro": "nodemon --exec node -r  ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node ./src/libraries/scrape/metro.lib.ts",
    "dev:clicon": "nodemon --exec node -r  ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node ./src/libraries/providers/climate.lib.ts",
    "nest:build": "nest build",
    "schema": "prisma generate",
    "migrate": "prisma migrate dev --name added_melon",
    "db:update": "prisma db push",
    "nest:build": "nest build"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "@prisma/client": "^4.13.0",
    "@types/express-session": "^1.17.7",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^6.1.5",
    "moment-timezone": "^0.5.43",
    "node-fetch": "^3.3.1",
    "node-schedule": "^2.1.1",
    "nodemon": "^2.0.22",
    "prisma": "^4.13.0",
    "puppeteer": "^19.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "set-interval-async": "^3.0.3",
    "source-map-support": "^0.5.21",
    "utf8": "^3.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^18.16.0",
    "@types/node-schedule": "^2.1.0",
    "@types/source-map-support": "^0.5.6",
    "@types/utf8": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.58.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.26.0",
    "prettier": "^2.8.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "ts-paths-esm-loader": "^1.4.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
